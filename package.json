{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Webpack template with loaders, plugins, ESLint, and more.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.2",
    "style-loader": "^4.0.0",
    "webpack-merge": "^6.0.1",
    "babel-loader": "^8.2.3",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "eslint": "^8.0.0",
    "eslint-webpack-plugin": "^2.5.0",
    "prettier": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^2.5.0",
    "file-loader": "^6.2.0"
